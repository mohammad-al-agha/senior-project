{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "strictNullChecks": false
  },
  "include": ["src/**/*"]
}
